OPENAI_API_KEY={OPENAI_API_KEY}
JUPYTER_SERVER=http://localhost:8888
JUPYTER_TOKEN=ebcec7fcf42f28baccfab1cbc07bfb3f
ENABLE_CHECKPOINTS=true
TOOL_ENABLED_ASK_USER=true
TOOL_ENABLED_RUN_CODE=true

# Authentication mode: 'alb' (ALB-level), 'app' (app-level), 'auto' (try both)
# Note: Only set this if using CognitoHeadersIdentityProvider. For local/opensource, leave unset to use default Jupyter token auth.
# BEAKER_AUTH_MODE=app

# AWS Cognito User Pool ID (optional - if set, enriches user info from Cognito)
# If not set, creates basic User object from user_id header
# Only needed if using CognitoHeadersIdentityProvider
# BEAKER_USER_POOL_ID=<your-cognito-user-pool-id>

# AWS Cognito Region (optional, auto-detected from user_pool_id if not set)
# Only needed if using CognitoHeadersIdentityProvider
# BEAKER_COGNITO_REGION=

# Custom header names for app-level auth (optional, defaults shown)
# Only needed if using CognitoHeadersIdentityProvider with app-level auth
# BEAKER_APP_USER_ID_HEADER=X-Beaker-User-Id
# BEAKER_APP_ACCESS_TOKEN_HEADER=X-Beaker-Access-Token
# BEAKER_APP_ID_TOKEN_HEADER=X-Beaker-Id-Token

# Whether to verify JWT signatures (default: true)
# Only needed if using CognitoHeadersIdentityProvider
# BEAKER_VERIFY_JWT=true
